		<dataSheet name="DeterministicIgnitionLocation" type="Input"/>
		<dataSheet name="DeterministicBurnCondition" type="Input"/>
		<dataSheet name="OutputOptionSpatial" type="Input"/>
		<dataSheet name="OutputFireStatistic" type="Both"/>
